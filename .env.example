VITE_BACKEND_URL=http://localhost:3001
VITE_GOOGLE_CLIENT_ID=

# Optional backend functions base URL.
# Example production: https://api.your-domain.com/api/functions
# Example local: http://localhost:3001/api/functions
VITE_BACKEND_FUNCTIONS_BASE_URL=http://localhost:3001/api/functions

# true = use backendClient.functions.invoke(endpoint)
# false = use fetch(baseUrl/endpoint) with Bearer token
VITE_USE_BACKEND_FUNCTIONS=true

VITE_TWITCH_CLIENT_ID=
VITE_TWITCH_REDIRECT_URI=http://localhost:5173/twitch/callback

# Dev-only fallback for admin route while you don't have user metadata is_admin configured.
VITE_ADMIN_UIDS=

